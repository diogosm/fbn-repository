/**** ETC ****/
.btn,
.btn-lg,
.btn-sm,
.btn-xs,
.btn-group-xs > .btn,
.btn-group-sm > .btn,
.btn-group-lg > .btn,
.input-group-lg > .input-group-btn > .btn,
.input-group-sm > .input-group-btn > .btn,
.breadcrumb,
.well,
.alert,
.panel,
.navbar-inverse .navbar-toggle,
#recent-submissions-carousel,
.panel-primary > .panel-heading {
    border-radius: 0;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
}

.bg-img-clear {
    background-image: -webkit-gradient(linear, left 0%, left 100%, from(#ffffff), to(#ffffff));
    background-image: -webkit-linear-gradient(top, #ffffff, 0%, #ffffff, 100%);
    background-image: -moz-linear-gradient(top, #ffffff 0%, #ffffff 100%);
    background-image: linear-gradient(to bottom, #ffffff 0%, #ffffff 100%);
    background-repeat: repeat-x;
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffffff', endColorstr='#ffffffff', GradientType=0);
}

.well {
    border-width: 1px 1px 1px 7px;
    background: #f7f7f9;
    border-color: #c9c9c9;
}

.panel-primary > .panel-heading {
    background-image: -webkit-gradient(linear, left 0%, left 100%, from(#5e96d2), to(#5e9ecc));
    background-image: -webkit-linear-gradient(top, #5e96d2, 0%, #5e9ecc, 100%);
    background-image: -moz-linear-gradient(top, #5e96d2 0%, #5e9ecc 100%);
    background-image: linear-gradient(to bottom, #5e96d2 0%, #5e9ecc 100%);
    background-repeat: repeat-x;
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#5e96d2', endColorstr='#5e9ecc', GradientType=0);
}

.highlight {
    background-color: #f7f7f9;
    border: 1px solid #E1E1E8;
    border-bottom: 7px solid #E1E1E8;
    margin-bottom: 14px;
    padding: 9px 14px;
}

.inline {
    display: inline;
}

a {
    /*color :#4796CD;*/
    color :#0074D9;
}

h1, h2, h3, h4, h5, h6 {
    /*color: #49b5d9;*/
    color: #001f3f;
    font-family: "pt-sans-caption","Source Sans Pro",sans-serif;
    font-weight: 600;
    margin: 15px 0;

}

h1 i, h2 i, h3 i, h4 i, h5 i, h6 i {
