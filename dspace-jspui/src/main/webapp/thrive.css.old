#sliding-popup {
	box-shadow: 0 0 15px rgba(0,0,0,0.2);
}

#sliding-popup > div {
	max-width: 960px;
	margin-left: auto;
	margin-right: auto;
	width: 100%;
}

#sliding-popup > div .popup-content {
	max-width: 100%;
	padding: 5px 15px;
}

#sliding-popup > div .popup-content #popup-buttons {
	float: none;
	max-width: 100%;
	margin-bottom: 0;
}

#sliding-popup > div .popup-content #popup-buttons button {
	height: 35px;
	background-color: #f3f5f6;
	color: #0b6dc5;
	border: 1px solid #0b6dc5;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	-moz-box-shadow: 0 2px 2px rgba(0,0,0,0.15);
	-webkit-box-shadow: 0 2px 2px rgba(0,0,0,0.15);
	box-shadow: 0 2px 2px rgba(0,0,0,0.15);
	cursor: pointer;
	font-family: Arial,sans-serif;
	font-size: 12px;
	font-weight: bold;
	margin: 0;
	padding: 0 12px;
	-webkit-appearance: none;
	text-shadow: none;
	margin-bottom: 5px;
}

#sliding-popup > div .popup-content #popup-buttons button:hover {
	background-color: #e0e0e0;
	background-image: none;
}

#sliding-popup > div .popup-content #popup-buttons button + button {
	box-shadow: none;
	border: medium none;
	background-color: transparent;
	color: #FFF;
	background-image: none;
}

#sliding-popup > div .popup-content #popup-buttons button + button:hover {
	box-shadow: none;
	border: medium none;
	text-decoration: underline;
	background-color: transparent;
	color: #FFF;
	background-image: none;
}

#sliding-popup > div .popup-content #popup-text {
	display: block;
